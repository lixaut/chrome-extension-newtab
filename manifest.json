{
  "manifest_version": 3,
  "name": "NewTaber",
  "version": "1.0.0",
  "description": "Override the newtab",
  "author": "wlkp9108@gmail.com",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js"
  }
}